<% include ./partials/header%>
<div class="container">
	<div class="row">
		<div class="col-sm-6 col-sm-offset-3">
			<form action="http://localhost:3000/users/register" method="POST" role="form">
				<h1>Register here</h1>
			
				<div class="form-group">
					
					<input name="name" type="text" class="form-control" id="" placeholder="Name">
				</div>
			
				<div class="form-group">
				
					<input name="email" type="text" class="form-control" id="" placeholder="email">
				</div>
			
				<div class="form-group">
					
					<input name="password" type="text" class="form-control" id="" placeholder="password">
				</div>
			
				
			
				<button type="submit" class="btn btn-primary">Register</button>
			</form>
		</div>
	</div>
</div>
<%include ./partials/footer%>
