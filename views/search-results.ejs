<% include ./partials/header%>
<div class="container">
	<div class="row">
		<%if(data.length === 0){%>
			<h1>Search not Found ..</h1>
		<%}else{%>
			
				<%data.forEach((prod)=>{%>
			<div class="col-sm-3">
				<a href="/product/<%=prod._id%>">
					<img src="<%=prod._source.image%>" alt="" class="img-responsive img-thumbnail">
				<h3><%=prod._source.name%></h3>
				<p><%=prod._source.price%></p>
				</a>
			</div>
			<%})%>
		<%}%>
	</div>
</div>
<%include ./partials/footer%>
